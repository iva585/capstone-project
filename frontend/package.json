{
  "name": "hamiti-frontend",
  "version": "0.0.1",
  "scripts": {
    "client:dev": "npx vite",
    "client:build": "npx tsc && vite build",
    "build": "npm run client:build && npm run storybook:build",
    "dev": "npx concurrently \\\"npm run client:dev\\\" \\\"npm run storybook\\\"",
    "serve": "npx vite preview",
    "storybook": "npx start-storybook -p 6006",
    "storybook:build": "npx build-storybook -o dist/storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-actions": "^6.4.3",
    "@storybook/addon-essentials": "^6.4.3",
    "@storybook/addon-links": "^6.4.3",
    "@storybook/react": "^6.4.3",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.4.0",
    "storybook-builder-vite": "^0.1.10",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2",
    "vite": "^2.6.14"
  },
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.2",
    "@mui/styled-engine": "^5.2.0",
    "@reduxjs/toolkit": "^1.6.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "redux": "^4.1.2"
  },
  "engines": {
    "node": "16.11.1",
    "npm": "8.0.0"
  }
}
